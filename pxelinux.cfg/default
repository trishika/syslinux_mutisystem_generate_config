DEFAULT vesamenu.c32

prompt 0

menu INCLUDE pxelinux.cfg/graphics.conf
MENU AUTOBOOT Starting Local System in # seconds

label bootlocal
  menu label No PXE Boot
  menu default
  localboot 0
  timeout 80
  TOTALTIMEOUT 9000

LABEL Setup Linux Menu
  MENU LABEL Setup Linux Menu
  KERNEL vesamenu.c32
  APPEND pxelinux.cfg/graphics.conf pxelinux.cfg/setup.menu

LABEL Live Menu
  MENU LABEL Live Menu
  KERNEL vesamenu.c32
  APPEND pxelinux.cfg/graphics.conf pxelinux.cfg/live.menu

LABEL Tools Menu
  MENU LABEL Tools Menu
  KERNEL vesamenu.c32
  APPEND pxelinux.cfg/graphics.conf pxelinux.cfg/tools.menu

LABEL hdt
        MENU LABEL HDT (Hardware Detection Tool)
        COM32 hdt.c32

LABEL cmd
        MENU LABEL CMD
        COM32 cmd.c32


LABEL reboot
        MENU LABEL Reboot
        COM32 reboot.c32

